{
  "name": "create_new_element",
  "version": "1.0.1",
  "description": "A simple HTML element crafting library",
  "scripts": {
    "type-check": "npx tsc",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "npx tsc --emitDeclarationOnly",
    "build:js": "npx babel src --out-dir dist --extensions \".ts,.tsx\" --presets minify",
    "test": "npx jest"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-typescript": "^7.10.4",
    "babel-preset-minify": "^0.5.1",
    "jest": "^26.4.0",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "HTML",
    "HTML Element",
    "modular",
    "functional"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YinkaEnoch/create_new_element.git"
  },
  "author": {
    "name": "Yinka Enoch Adedokun",
    "email": "adedokunyinkaenoch@gmail.com",
    "url": "https://yinkaenoch.github.io"
  },
  "license": "ISC",
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/YinkaEnoch/create_new_element/issues"
  },
  "homepage": "https://github.com/YinkaEnoch/create_new_element#readme"
}
